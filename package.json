{
  "name": "starbot-api",
  "version": "1.0.0",
  "description": "Starbot API - The Brain (model routing, tools, storage)",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "api:tui": "tsx scripts/api-tui.ts"
  },
  "keywords": [
    "starbot",
    "ai",
    "llm",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.700.0",
    "@azure/openai": "^2.0.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/websocket": "^11.0.1",
    "@google-cloud/vertexai": "^1.9.1",
    "@prisma/client": "^5.22.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.3.1",
    "fastify": "^5.7.4",
    "jsonwebtoken": "^9.0.3",
    "mathjs": "^15.1.1",
    "nanoid": "^5.0.0",
    "openai": "^6.22.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.19.11",
    "@vitest/coverage-v8": "^4.0.18",
    "epub-maker": "^1.2.0",
    "pino-pretty": "^13.1.3",
    "prisma": "^5.22.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  }
}
